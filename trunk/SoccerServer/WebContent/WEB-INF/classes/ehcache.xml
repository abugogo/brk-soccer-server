<?xml version="1.0" encoding="UTF-8"?>

<ehcache name="TheCache">
    <diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="0" timeToLiveSeconds="0" overflowToDisk="false"
		maxElementsOnDisk="0" diskPersistent="false"
		diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LRU" />
	<cache name="stats_cache" maxElementsInMemory="1000" maxElementsOnDisk="0"
		eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LFU">
	</cache>
</ehcache>
